{
  "expo": {
    "name": "Kin",
    "slug": "kin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/kincompletelogo.png",
    "scheme": "kin",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/kincompletelogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#2A1F1F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kinfamily.app",
      "infoPlist": {
        "NSHealthShareUsageDescription": "Kin needs access to your step count data to track your daily fitness goals and display your progress.",
        "NSHealthUpdateUsageDescription": "Kin may update your health data to record your workouts.",
        "UIBackgroundModes": [
          "fetch",
          "fetch"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.developer.healthkit": true,
        "com.apple.developer.healthkit.access": [
          "health-records",
          "health-records"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "010fc0ff-d630-4f3e-a7dd-6c36f1833a1a"
      }
    }
  }
}
